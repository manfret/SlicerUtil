<UserControl x:Class="Aura.Views.SettingsItemProfileView"
                                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                                xmlns:util="clr-namespace:Aura.Controls.Util"
                                xmlns:viewModels="clr-namespace:Aura.ViewModels"
                                xmlns:primitive="clr-namespace:Aura.Controls.Primitive"
                                xmlns:localization="clr-namespace:Aura.Themes.Localization"
                                xmlns:resx="clr-namespace:Aura.Controls.Resources"
                                xmlns:val="clr-namespace:Settings.ValidValues;assembly=Settings"
                                xmlns:memento="clr-namespace:Settings.Memento;assembly=Settings"
                                xmlns:validationRules="clr-namespace:Aura.Controls.ValidationRules"
                                mc:Ignorable="d"
                                x:Name="UserControl"
                                d:DataContext="{d:DesignInstance viewModels:SettingsItemVMProfile, IsDesignTimeCreatable=True}">
    <UserControl.Resources>
        <util:InvertBooleanConverter x:Key="InvertBoolConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <util:BooleanToVisibilityInvertConverter x:Key="BooleanToVisibilityInvertConverter" />
        <ScaleTransform x:Key="CheckBoxTransform" CenterX="0.5" CenterY="0.5"
                        ScaleX="1.18" ScaleY="1.18" />
        <util:FiberInfillTypeToVisibilityConverter x:Key="FiberInfillTypeToVisibilityConverter" />
        <util:FiberInfillTypeInvertToVisibilityConverter x:Key="FiberInfillTypeInvertToVisibilityConverter" />
    </UserControl.Resources>
    <Border Padding="0 0 0 10" Background="#f2f2f2" Width="380">
            <StackPanel Orientation="Vertical">
                <Border x:Name="BorderProfileName"
                        Style="{StaticResource SimpleRowStyleSettings}">
                    <DockPanel LastChildFill="True">
                        <primitive:QuestionedTextBlock
                            Caption="{x:Static localization:Common_en_EN.Name}"
                            DockPanel.Dock="Left"
                            CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                        <TextBox x:Name="ProfileName"
                                 DockPanel.Dock="Right"
                                 Style="{DynamicResource TextBoxSettingsWithoutWidth}"
                                 IsReadOnly="{Binding Model.IsAnisoprintApproved}">
                            <TextBox.Text>
                                <Binding Path="Model.Name" Mode="TwoWay" UpdateSourceTrigger="LostFocus">
                                    <Binding.ValidationRules>
                                        <validationRules:NameValidator/>
                                    </Binding.ValidationRules>
                                </Binding>
                            </TextBox.Text>
                    </TextBox>
                    </DockPanel>
                </Border>
                <StackPanel x:Name="SPLayupRule" Orientation="Vertical">
                    <Border x:Name="BorderAllowGenerateFiber"
                            Style="{StaticResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <CheckBox x:Name="AllowGenerateFiber"
                                      DockPanel.Dock="Right"
                                      RenderOptions.BitmapScalingMode="HighQuality"
                                      HorizontalAlignment="Right"
                                      SnapsToDevicePixels="True"
                                      VerticalAlignment="Center"
                                      Style="{DynamicResource CheckBoxToggleStyle}"
                                      IsChecked="{Binding Model.GlobalSettingsMemento.AllowGenerateFiber, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      LayoutTransform="{StaticResource CheckBoxTransform}" />
                            <primitive:QuestionedTextBlock
                                x:Name="PART_AllowGenerateFiber"
                                Caption="{x:Static localization:Global_en_EN.AllowGenerateFiber}"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                TipLabelText="{x:Static localization:Global_en_EN.AllowGenerateFiber}"
                                TipDescription="{x:Static resx:SettingsTipsResorces.ProfilePartAllowGenerateFiber}" />
                        </DockPanel>
                    </Border>
                </StackPanel>
                <StackPanel x:Name="SPLayerThicknesses" Orientation="Vertical">
                    <primitive:TextDivider x:Name="DivLayerThickness"
                                           Style="{DynamicResource DividerRowStyle}"
                                           VerticalAlignment="Bottom"
                                           Text="{x:Static localization:Global_en_EN.HEADER_LAYER_THICKNESS}" />
                    <Border x:Name="BorderMacroLayerHeight"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="PartMacroLayerHeight"
                                                    DockPanel.Dock="Right"
                                                    BoxType="DOUBLES"
                                                    Minimum="{Binding Source={x:Static val:SGlobal.Default}, Path=MacroLayerHeightMin}"
                                                    Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=MacroLayerHeightMax}"
                                                    Value="{Binding Model.GlobalSettingsMemento.MacroLayerHeight, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}"
                                                    TextStyle="{DynamicResource TextBoxSettings}" />
                            <primitive:QuestionedTextBlock
                                Caption="{x:Static localization:Global_en_EN.MacroLayerHeight}"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                TipLabelText="{x:Static localization:Global_en_EN.MacroLayerHeight}"
                                TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessMacroLayerHeight}"
                                TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileLayerThicknessMacroLayerHeight}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInset0RatioToMacro"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:QuestionedTextBlock x:Name="QTBInset0RatioToMacro"
                                                           Caption="{x:Static localization:Inset0_en_EN.RatioToMacro}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           DockPanel.Dock="Top"
                                                           TipLabelText="{x:Static localization:Inset0_en_EN.RatioToMacro}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessInset0RatioToMacro}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileExternalShellThickness}" />
                            <primitive:SliderWithODivide
                                x:Name="SliOInset0PRatioToMacro"
                                Minimum="1"
                                Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=Inset0RatioToMacroMax}"
                                TickFrequency="1"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                Divi="{Binding Model.GlobalSettingsMemento.MacroLayerHeight}"
                                IsEnabled="{Binding Model.IsAnisoprintApproved, Converter={StaticResource InvertBoolConverter}}"
                                Value="{Binding Model.GlobalSettingsMemento.Inset0RatioToMacro, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXPRatioToMacro"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:QuestionedTextBlock x:Name="QTBInsetXPRatioToMacro"
                                                           Caption="{x:Static localization:InsetPlastic_en_EN.RatioToMacro}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           DockPanel.Dock="Top"
                                                           TipLabelText="{x:Static localization:InsetPlastic_en_EN.RatioToMacro}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessInsetXPRatioToMacro}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileLayerThicknessInsetXPInfill}" />
                            <primitive:SliderWithODivide
                                x:Name="SliOInsetXPRatioToMacro"
                                Minimum="1"
                                Maximum="{Binding Model.GlobalSettingsMemento.Inset0RatioToMacro}"
                                TickFrequency="1"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                IsEnabled="{Binding Model.IsAnisoprintApproved, Converter={StaticResource InvertBoolConverter}}"
                                Divi="{Binding Model.GlobalSettingsMemento.MacroLayerHeight}"
                                Value="{Binding Model.GlobalSettingsMemento.InsetXPRatioToMacro, Mode=TwoWay}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInfillRatioToMacro"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:QuestionedTextBlock x:Name="QTBInfillRatioToMacro"
                                                           Caption="{x:Static localization:InfillPlasticSolid_en_EN.RatioToMacro}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           DockPanel.Dock="Top"
                                                           TipLabelText="{x:Static localization:InfillPlasticSolid_en_EN.RatioToMacro}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessInfillRatioToMacro}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileLayerThicknessInsetXPInfill}" />
                            <primitive:SliderWithODivide
                                x:Name="SliOInfillRatioToMacro"
                                Minimum="1"
                                Maximum="{Binding Model.GlobalSettingsMemento.Inset0RatioToMacro}"
                                TickFrequency="1"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                Divi="{Binding Model.GlobalSettingsMemento.MacroLayerHeight}"
                                IsEnabled="{Binding Model.IsAnisoprintApproved, Converter={StaticResource InvertBoolConverter}}"
                                Value="{Binding Model.GlobalSettingsMemento.InfillRatioToMacro, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderSSThickRatioToMacro"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:QuestionedTextBlock x:Name="QTBSSThickRatioToMacro"
                                                           Caption="{x:Static localization:Support_en_EN.RatioToMacroThick}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           DockPanel.Dock="Top"
                                                           TipLabelText="{x:Static localization:Support_en_EN.RatioToMacroThick}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessSSThickRatioToMacro}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileLayerThicknessSupport}" />
                            <primitive:SliderWithODivide
                                x:Name="SliOSSThickRatioToMacro"
                                Minimum="1"
                                Maximum="{Binding Model.GlobalSettingsMemento.SupportThinRatioToMacro}"
                                TickFrequency="1"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                Divi="{Binding Model.GlobalSettingsMemento.MacroLayerHeight}"
                                IsEnabled="{Binding Model.IsAnisoprintApproved, Converter={StaticResource InvertBoolConverter}}"
                                Value="{Binding Model.GlobalSettingsMemento.SupportThickRatioToMacro, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderSSThinRatioToMacro"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:QuestionedTextBlock x:Name="QTBSSThinRatioToMacro"
                                                           Caption="{x:Static localization:Support_en_EN.RatioToMacroThin}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           DockPanel.Dock="Top"
                                                           TipLabelText="{x:Static localization:Support_en_EN.RatioToMacroThin}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileLayerThicknessSSThinRatioToMacro}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileLayerThicknessSupport}" />
                            <primitive:SliderWithODivide
                                x:Name="SliOSSThinRatioToMacro"
                                Minimum="1"
                                Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=SSThinRatioToMacroMax}"
                                TickFrequency="1"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                Divi="{Binding Model.GlobalSettingsMemento.MacroLayerHeight}"
                                IsEnabled="{Binding Model.IsAnisoprintApproved, Converter={StaticResource InvertBoolConverter}}"
                                Value="{Binding Model.GlobalSettingsMemento.SupportThinRatioToMacro, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </DockPanel>
                    </Border>
                </StackPanel>
                <StackPanel x:Name="SPFirstLayer" Orientation="Vertical">
                    <primitive:TextDivider x:Name="DivFirstLayer"
                                           Style="{DynamicResource DividerRowStyle}"
                                           VerticalAlignment="Bottom"
                                           Text="{x:Static localization:Global_en_EN.HEADER_FIRST_LAYER}" />
                    <Border x:Name="BorderFLEWCoeffToNormal"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileFLEWCoeffToNormal"
                                                    DockPanel.Dock="Right"
                                                    BoxType="DOUBLES"
                                                    Minimum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLEWCoeffToNormalMin}"
                                                    Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLEWCoeffToNormalMax}"
                                                    Value="{Binding Model.GlobalSettingsMemento.FLEWCoeffToNormal, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock
                                Caption="{x:Static localization:Global_en_EN.FLEWCoeffToNormal}"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                TipLabelText="{x:Static localization:Global_en_EN.FLEWCoeffToNormal}"
                                TipDescription="{x:Static resx:SettingsTipsResorces.ProfileFLEWCoeffToNormal}"
                                TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileFLCoeffToNormals}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderFLThickness"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileFLThickness"
                                                    DockPanel.Dock="Right"
                                                    BoxType="DOUBLES"
                                                    Minimum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLThicknessMin}"
                                                    Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLThicknessMax}"
                                                    Value="{Binding Model.GlobalSettingsMemento.FLThickness, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:Global_en_EN.FLThickness}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:Global_en_EN.FLThickness}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileFLThickness}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileFLThickness}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderFLSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileFLSpeed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLSpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SGlobal.Default}, Path=FLSpeedMax}"
                                                    Value="{Binding Model.GlobalSettingsMemento.FLSpeed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:Global_en_EN.FLSpeed}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:Global_en_EN.FLSpeed}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileFLSpeed}" />
                        </DockPanel>
                    </Border>
                </StackPanel>
                <StackPanel x:Name="SPInset0" Orientation="Vertical">
                    <primitive:TextDivider x:Name="DivInset0"
                                           Style="{DynamicResource DividerRowStyle}"
                                           VerticalAlignment="Bottom"
                                           Text="{x:Static localization:Global_en_EN.HEADER_EXTERNAL_SHELL}" />
                    <Border x:Name="BorderInset0EWCoeff"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInset0EWCoeff"
                                                    DockPanel.Dock="Right"
                                                    BoxType="DOUBLES"
                                                    Minimum="{Binding Source={x:Static val:SInset0.Default}, Path=EWCoeffMin}"
                                                    Maximum="{Binding Source={x:Static val:SInset0.Default}, Path=EWCoeffMax}"
                                                    Value="{Binding Model.Inset0PlasticSettingsMemento.EWCoeff, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:Inset0_en_EN.EWCoeff}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:Inset0_en_EN.EWCoeff}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileInset0EWCoeff}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileExtrusionCoefficient}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInset0Speed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInset0Inset0Speed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SInset0.Default}, Path=SpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SInset0.Default}, Path=SpeedMax}"
                                                    Value="{Binding Model.Inset0PlasticSettingsMemento.Speed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:Inset0_en_EN.Speed}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:Inset0_en_EN.Speed}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileInset0Speed}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileExternalShellPrintingSpeed}" />
                        </DockPanel>
                    </Border>
                </StackPanel>
                <StackPanel x:Name="SPInsetXP" Orientation="Vertical">
                    <primitive:TextDivider x:Name="DivInsetXP"
                                           Style="{DynamicResource DividerRowStyle}"
                                           VerticalAlignment="Bottom"
                                           Text="{x:Static localization:Global_en_EN.HEADER_PLASTIC_PERIMETERS}" />
                    <Border x:Name="BorderInsetXPEWCoeff"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXPEWCoeff"
                                                    DockPanel.Dock="Right"
                                                    BoxType="DOUBLES"
                                                    Minimum="{Binding Source={x:Static val:SInsetP.Default}, Path=EWCoeffMin}"
                                                    Maximum="{Binding Source={x:Static val:SInsetP.Default}, Path=EWCoeffMax}"
                                                    Value="{Binding Model.InsetXPlasticSettingsMemento.EWCoeff, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:InsetPlastic_en_EN.EWCoeff}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:InsetPlastic_en_EN.EWCoeff}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileInsetXPEWCoeff}"
                                                           TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgProfileExtrusionCoefficient}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXPSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXPInset0Speed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SInsetP.Default}, Path=SpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SInsetP.Default}, Path=SpeedMax}"
                                                    Value="{Binding Model.InsetXPlasticSettingsMemento.Speed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock Caption="{x:Static localization:InsetPlastic_en_EN.Speed}"
                                                           CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                                           TipLabelText="{x:Static localization:InsetPlastic_en_EN.Speed}"
                                                           TipDescription="{x:Static resx:SettingsTipsResorces.ProfileInsetXPSpeed}" />
                        </DockPanel>
                    </Border>
                </StackPanel>
                <StackPanel x:Name="SPFiberSettings" Orientation="Vertical"
                            Visibility="{Binding Model.GlobalSettingsMemento.AllowGenerateFiber, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <primitive:TextDivider x:Name="DivF"
                                           Style="{DynamicResource DividerRowStyle}"
                                           VerticalAlignment="Bottom"
                                           Text="{x:Static localization:Global_en_EN.HEADER_FIBER_PRINTING}" />
                    <Border x:Name="BorderExtrusionMultiplier"
                            Style="{StaticResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox
                                x:Name="MaterialExtrusionMultiplier"
                                DockPanel.Dock="Right"
                                BoxType="DOUBLES"
                                Minimum="{Binding Source={x:Static val:SF.Default}, Path=ExtrusionMultiplierPMin}"
                                Maximum="{Binding Source={x:Static val:SF.Default}, Path=ExtrusionMultiplierPMax}"
                                Value="{Binding Mode=TwoWay, Path=Model.FiberPrintingSettingsMemento.ExtrusionMultiplierP, UpdateSourceTrigger=PropertyChanged}"
                                TextStyle="{DynamicResource TextBoxSettings}"
                                IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock
                                Caption="{x:Static localization:Fiber_en_EN.FlowMultiplier}"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}"
                                TipLabelText="{x:Static localization:Fiber_en_EN.FlowMultiplier}"
                                TipDescription="{x:Static resx:SettingsTipsResorces.FiberExtrusionMultiplier}"
                                TipImageBitmap="{x:Static resx:SettingsTipsResorces.ImgFiberPlasticFlowMultiplier}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderDoZHopAfterCut"
                            Style="{StaticResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <CheckBox x:Name="DoZHopAfterCut"
                                      DockPanel.Dock="Right"
                                      RenderOptions.BitmapScalingMode="HighQuality"
                                      HorizontalAlignment="Right"
                                      SnapsToDevicePixels="True"
                                      VerticalAlignment="Center"
                                      Style="{DynamicResource CheckBoxToggleStyle}"
                                      IsChecked="{Binding Model.FiberPrintingSettingsMemento.DoZHopAfterCut, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      LayoutTransform="{StaticResource CheckBoxTransform}" />
                            <primitive:QuestionedTextBlock
                                x:Name="PART_DoZHopAfterCut"
                                Caption="{x:Static localization:FiberPrinting_en_EN.DoZHopAfterCut}"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXFStartMaxSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXFStartMaxSpeed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SF.Default}, Path=StartMaxSpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SF.Default}, Path=StartMaxSpeedMax}"
                                                    Value="{Binding Model.FiberPrintingSettingsMemento.StartMaxSpeed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock
                                Caption="StartMaxSpeed(Developer)"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXFStartMinSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXFStartMinSpeed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SF.Default}, Path=StartMinSpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SF.Default}, Path=StartMinSpeedMax}"
                                                    Value="{Binding Model.FiberPrintingSettingsMemento.StartMinSpeed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock
                                Caption="StartMinSpeed(Developer)"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXFStartMinLimitSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXFStartMinLimitSpeed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
                                                    Minimum="{Binding Source={x:Static val:SF.Default}, Path=StartMinLimitSpeedMin}"
                                                    Maximum="{Binding Source={x:Static val:SF.Default}, Path=StartMinLimitSpeedMax}"
                                                    Value="{Binding Model.FiberPrintingSettingsMemento.StartMinLimitSpeed, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                    TextStyle="{DynamicResource TextBoxSettings}"
                                                    IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                            <primitive:QuestionedTextBlock
                                Caption="StartMinSpeedLimit(Developer)"
                                CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                        </DockPanel>
                    </Border>
                    <Border x:Name="BorderInsetXFNormalMaxSpeed"
                            Style="{DynamicResource SimpleRowStyleSettings}">
                        <DockPanel LastChildFill="True">
                            <primitive:TypedTextBox x:Name="ProfileInsetXFNormalMaxSpeed"
                                                    DockPanel.Dock="Right"
                                                    BoxType="INTEGERS"
