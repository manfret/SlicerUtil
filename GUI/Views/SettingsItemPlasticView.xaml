                                                BoxType="DOUBLES"
                                                Minimum="{Binding Source={x:Static val:SMaterialP.Default}, Path=SpoolWeightMin}"
                                                Maximum="{Binding Source={x:Static val:SMaterialP.Default}, Path=SpoolWeightMax}"
                                                Value="{Binding Mode=TwoWay, Path=Model.SpoolWeight, UpdateSourceTrigger=PropertyChanged}"
                                                TextStyle="{DynamicResource TextBoxSettings}"
                                                IsReadOnly="{Binding Model.IsAnisoprintApproved}" />
                    <primitive:QuestionedTextBlock
                            Caption="{x:Static localization:Plastic_en_EN.SpoolWeight}"
                            CaptionStyle="{DynamicResource LabelTextBlockSettings}" />
                </DockPanel>
            </Border>
            <Border x:Name="BorderVersion"
                        Style="{StaticResource SimpleRowStyleSettings}">
                <DockPanel LastChildFill="True">
                    <primitive:TypedTextBox x:Name="MaterialVersion"
